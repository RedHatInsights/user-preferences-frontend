{
  "name": "user-preferences-frontend",
  "version": "1.1.0",
  "engines": {
    "node": ">=15.14.0",
    "npm": ">=7.24.2"
  },
  "private": false,
  "dependencies": {
    "@babel/runtime": "^7.28.6",
    "@data-driven-forms/mui-component-mapper": "^4.1.15",
    "@data-driven-forms/pf4-component-mapper": "^4.1.14",
    "@data-driven-forms/react-form-renderer": "^4.1.13",
    "@openshift/dynamic-plugin-sdk": "^4.0.0",
    "@patternfly/react-component-groups": "^6.4.0",
    "@patternfly/react-core": "^6.4.1",
    "@patternfly/react-icons": "^6.4.0",
    "@patternfly/react-table": "^6.4.1",
    "@redhat-cloud-services/frontend-components": "^6.1.1",
    "@redhat-cloud-services/frontend-components-notifications": "^6.1.43",
    "@redhat-cloud-services/frontend-components-utilities": "^6.1.1",
    "@scalprum/react-core": "^0.11.1",
    "classnames": "^2.5.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-redux": "^7.2.9",
    "react-router-dom": "^6.30.3",
    "redux": "^4.2.1",
    "redux-logger": "^3.0.6",
    "redux-promise-middleware": "^6.2.0"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "@testing-library/jest-dom/extend-expect"
    ],
    "coverageDirectory": "./coverage/",
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/**/stories/*"
    ],
    "setupFiles": [
      "<rootDir>/config/setupTests.js"
    ],
    "roots": [
      "<rootDir>/src/"
    ],
    "moduleNameMapper": {
      "\\.(css|scss)$": "identity-obj-proxy"
    },
    "testURL": "http://localhost:5000/"
  },
  "devDependencies": {
    "@babel/core": "^7.29.0",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-object-rest-spread": "^7.20.7",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.29.0",
    "@babel/preset-env": "^7.29.0",
    "@babel/preset-flow": "^7.27.1",
    "@babel/preset-react": "^7.28.5",
    "@babel/preset-typescript": "^7.28.5",
    "@patternfly/patternfly": "^6.4.0",
    "@redhat-cloud-services/eslint-config-redhat-cloud-services": "^1.3.0",
    "@redhat-cloud-services/tsc-transform-imports": "^1.0.55",
    "@storybook/addon-docs": "^9.1.19",
    "@storybook/addon-webpack5-compiler-swc": "^3.0.0",
    "@storybook/react-webpack5": "^9.1.19",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^14.3.1",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "axios-mock-adapter": "^1.22.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-plugin-lodash": "^3.3.4",
    "chromatic": "^15.2.0",
    "cross-fetch": "^3.2.0",
    "css-loader": "^5.2.7",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^8.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-storybook": "^9.1.19",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.6.3",
    "msw": "^2.12.10",
    "msw-storybook-addon": "^2.0.6",
    "npm-run-all": "^4.1.5",
    "prop-types": "^15.8.1",
    "redux-mock-store": "^1.5.5",
    "sass-loader": "^11.1.1",
    "storybook": "^9.1.19",
    "stylelint": "^13.13.1",
    "stylelint-config-recommended-scss": "^4.3.0",
    "stylelint-scss": "^3.21.0",
    "ts-patch": "^3.3.0",
    "typescript": "^5.9.3",
    "webpack-bundle-analyzer": "^4.10.2"
  },
  "transformIgnorePatterns": [
    "/node_modules/(?!@redhat-cloud-services)"
  ],
  "scripts": {
    "build": "fec build",
    "test": "jest --verbose --transformIgnorePatterns 'node_modules/(?!my-library-dir)/'",
    "lint": "npm-run-all lint:*",
    "lint:js": "eslint config src",
    "lint:js:fix": "eslint config src --fix",
    "lint:sass": "stylelint 'src/**/*.scss' --config .stylelintrc.json",
    "server:ctr": "node src/server/generateServerKey.js",
    "start": "HOT=true fec dev",
    "deploy": "npm-run-all build lint test",
    "verify": "npm-run-all build lint test",
    "nightly": "npm run deploy",
    "postinstall": "ts-patch install",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "insights": {
    "appname": "user-preferences"
  },
  "msw": {
    "workerDirectory": [
      "static"
    ]
  }
}
