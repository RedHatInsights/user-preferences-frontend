{
  "name": "user-preferences-frontend",
  "version": "1.1.0",
  "engines": {
    "node": ">=15.0.0",
    "npm": ">=7.0.0"
  },
  "private": false,
  "dependencies": {
    "@babel/runtime": "^7.13.10",
    "@data-driven-forms/pf4-component-mapper": "^3.22.3",
    "@data-driven-forms/react-form-renderer": "^3.22.3",
    "@patternfly/react-component-groups": "^5.1.0",
    "@patternfly/react-core": "^5.2.3",
    "@patternfly/react-icons": "^5.2.1",
    "@patternfly/react-table": "^5.2.4",
    "@redhat-cloud-services/frontend-components": "^4.2.5",
    "@redhat-cloud-services/frontend-components-notifications": "^4.1.0",
    "@redhat-cloud-services/frontend-components-utilities": "^4.0.7",
    "@scalprum/react-core": "^0.7.1",
    "classnames": "^2.5.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^7.2.9",
    "react-router-dom": "^6.22.3",
    "redux": "^4.0.5",
    "redux-logger": "^3.0.6",
    "redux-promise-middleware": "^6.1.2"
  },
  "jest": {
    "coverageDirectory": "./coverage/",
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/**/stories/*"
    ],
    "setupFiles": [
      "<rootDir>/config/setupTests.js"
    ],
    "roots": [
      "<rootDir>/src/"
    ],
    "moduleNameMapper": {
      "\\.(css|scss)$": "identity-obj-proxy"
    },
    "testEnvironmentOptions": {
      "url": "http://localhost:5000/"
    }
  },
  "devDependencies": {
    "@babel/core": "^7.13.10",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.13.8",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.13.10",
    "@babel/preset-env": "^7.13.10",
    "@babel/preset-flow": "^7.12.13",
    "@babel/preset-react": "^7.12.13",
    "@redhat-cloud-services/eslint-config-redhat-cloud-services": "^2.0.3",
    "@redhat-cloud-services/frontend-components-config": "^6.0.11",
    "@redhat-cloud-services/tsc-transform-imports": "^1.0.8",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.2",
    "@typescript-eslint/eslint-plugin": "^7.3.1",
    "@typescript-eslint/parser": "^7.3.1",
    "axios-mock-adapter": "^1.19.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-plugin-lodash": "^3.3.4",
    "cross-fetch": "^3.1.0",
    "css-loader": "^6.10.0",
    "eslint": "^8.9.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react": "^7.34.1",
    "eslint-webpack-plugin": "^4.1.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "npm-run-all": "^4.1.5",
    "prop-types": "^15.7.2",
    "redux-mock-store": "^1.5.4",
    "sass-loader": "^14.1.1",
    "stylelint": "^16.2.1",
    "stylelint-config-recommended-scss": "^14.0.0",
    "stylelint-scss": "^6.2.1",
    "ts-patch": "^3.1.2",
    "typescript": "^5.4.3",
    "webpack-bundle-analyzer": "^4.4.0"
  },
  "transformIgnorePatterns": [
    "/node_modules/(?!@redhat-cloud-services)"
  ],
  "scripts": {
    "build": "fec build",
    "test": "jest --verbose",
    "lint": "npm-run-all lint:*",
    "lint:js": "eslint config src",
    "lint:js:fix": "eslint config src --fix",
    "lint:sass": "stylelint 'src/**/*.scss' --config .stylelintrc.json",
    "server:ctr": "node src/server/generateServerKey.js",
    "start": "HOT=true fec dev",
    "deploy": "npm-run-all build lint test",
    "verify": "npm-run-all build lint test",
    "nightly": "npm run deploy",
    "postinstall": "ts-patch install"
  },
  "insights": {
    "appname": "user-preferences"
  }
}
