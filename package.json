{
  "name": "user-preferences-frontend",
  "version": "1.1.0",
  "engines": {
    "node": ">=15.0.0",
    "npm": ">=7.0.0"
  },
  "private": false,
  "dependencies": {
    "@babel/runtime": "^7.13.10",
    "@data-driven-forms/mui-component-mapper": "^3.20.13",
    "@data-driven-forms/pf4-component-mapper": "^3.19.0",
    "@data-driven-forms/react-form-renderer": "^3.19.0",
    "@openshift/dynamic-plugin-sdk": "^4.0.0",
    "@patternfly/react-core": "^5.3.0",
    "@patternfly/react-icons": "^5.3.0",
    "@patternfly/react-table": "^5.1.1",
    "@patternfly/react-component-groups": "^5.2.0-prerelease.4",
    "@redhat-cloud-services/frontend-components": "^4.2.2",
    "@redhat-cloud-services/frontend-components-notifications": "^4.1.0",
    "@redhat-cloud-services/frontend-components-utilities": "^4.0.2",
    "@scalprum/react-core": "^0.7.0",
    "classnames": "^2.3.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.1.3",
    "react-router-dom": "^6.21.1",
    "redux": "^4.2.1",
    "redux-logger": "^3.0.6",
    "redux-promise-middleware": "^6.2.0"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "@testing-library/jest-dom/extend-expect"
    ],
    "coverageDirectory": "./coverage/",
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/**/stories/*"
    ],
    "setupFiles": [
      "<rootDir>/config/setupTests.js"
    ],
    "roots": [
      "<rootDir>/src/"
    ],
    "moduleNameMapper": {
      "\\.(css|scss)$": "identity-obj-proxy"
    },
    "testURL": "http://localhost:5000/"
  },
  "devDependencies": {
    "@babel/core": "^7.13.10",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.13.8",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.13.10",
    "@babel/preset-env": "^7.13.10",
    "@babel/preset-flow": "^7.12.13",
    "@babel/preset-react": "^7.12.13",
    "@redhat-cloud-services/eslint-config-redhat-cloud-services": "^1.3.0",
    "@redhat-cloud-services/frontend-components-config": "^4.7.3",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^14.1.2",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "@types/jest": "^29.5.11",
    "@types/react": "^18.2.46",
    "@types/react-dom": "^18.2.18",
    "axios-mock-adapter": "^1.19.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-plugin-lodash": "^3.3.4",
    "cross-fetch": "^3.1.0",
    "css-loader": "^5.1.3",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-react": "^7.33.2",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.6.3",
    "npm-run-all": "^4.1.5",
    "prop-types": "^15.7.2",
    "redux-mock-store": "^1.5.4",
    "sass-loader": "^11.0.1",
    "stylelint": "^13.12.0",
    "stylelint-config-recommended-scss": "^4.2.0",
    "stylelint-scss": "^3.19.0",
    "typescript": "^5.3.3",
    "webpack-bundle-analyzer": "^4.4.0"
  },
  "transformIgnorePatterns": [
    "/node_modules/(?!@redhat-cloud-services)"
  ],
  "scripts": {
    "build": "fec build",
    "test": "jest --verbose",
    "lint": "npm-run-all lint:*",
    "lint:js": "eslint config src",
    "lint:js:fix": "eslint config src --fix",
    "lint:sass": "stylelint 'src/**/*.scss' --config .stylelintrc.json",
    "server:ctr": "node src/server/generateServerKey.js",
    "start": "HOT=true fec dev",
    "deploy": "npm-run-all build lint test",
    "verify": "npm-run-all build lint test",
    "nightly": "npm run deploy"
  },
  "insights": {
    "appname": "user-preferences"
  }
}
