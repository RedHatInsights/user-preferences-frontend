{
  "name": "insights-frontend-starter-app",
  "version": "1.1.0",
  "private": false,
  "dependencies": {
    "@babel/runtime": "^7.8.7",
    "@data-driven-forms/pf4-component-mapper": "^2.18.2",
    "@data-driven-forms/react-form-renderer": "^2.18.2",
    "@patternfly/patternfly": "^4.42.2",
    "@patternfly/react-core": "4.47.0",
    "@patternfly/react-table": "^4.16.20",
    "@redhat-cloud-services/frontend-components": "^2.5.1",
    "@redhat-cloud-services/frontend-components-notifications": "^2.2.3",
    "@redhat-cloud-services/frontend-components-utilities": "^2.2.8",
    "babel-plugin-transform-imports": "^2.0.0",
    "classnames": "^2.2.6",
    "core-js": "^3.6.5",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-redux": "^7.2.1",
    "react-router-dom": "^5.1.2",
    "redux": "^4.0.5",
    "redux-logger": "^3.0.6",
    "redux-promise-middleware": "^6.1.2"
  },
  "jest": {
    "coverageDirectory": "./coverage/",
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/**/stories/*"
    ],
    "setupFiles": [
      "<rootDir>/config/setupTests.js"
    ],
    "roots": [
      "<rootDir>/src/"
    ],
    "moduleNameMapper": {
      "\\.(css|scss)$": "identity-obj-proxy"
    },
    "testURL": "http://localhost:5000/"
  },
  "devDependencies": {
    "@babel/core": "^7.8.7",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.8.3",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@babel/preset-env": "^7.10.2",
    "@babel/preset-flow": "^7.8.3",
    "@babel/preset-react": "^7.8.3",
    "@redhat-cloud-services/eslint-config-redhat-cloud-services": "0.0.1",
    "@redhat-cloud-services/frontend-components-config": "3.0.0-beta12",
    "axios-mock-adapter": "^1.18.1",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^25.1.0",
    "babel-plugin-dual-import": "^1.2.1",
    "babel-plugin-lodash": "^3.3.4",
    "cross-fetch": "^3.0.6",
    "css-loader": "^5.0.1",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "enzyme-to-json": "^3.4.4",
    "eslint": "^7.15.0",
    "eslint-config-prettier": "^7.0.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-react": "^7.19.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^25.1.0",
    "node-sass": "^4.13.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^7.0.27",
    "prop-types": "^15.7.2",
    "sass-loader": "^10.1.1",
    "stylelint": "^13.2.1",
    "stylelint-config-recommended-scss": "^4.2.0",
    "stylelint-scss": "^3.14.2",
    "webpack": "^5.10.0",
    "webpack-bundle-analyzer": "^4.2.0",
    "webpack-cli": "^4.2.0"
  },
  "transformIgnorePatterns": [
    "/node_modules/(?!@redhat-cloud-services)"
  ],
  "scripts": {
    "build": "webpack --config config/prod.webpack.config.js",
    "test": "jest --verbose",
    "lint": "npm-run-all lint:*",
    "lint:js": "eslint config src",
    "lint:js:fix": "eslint config src --fix",
    "lint:sass": "stylelint 'src/**/*.scss' --config .stylelintrc.json",
    "prod": "NODE_ENV=production webpack serve --config config/dev.webpack.config.js",
    "server:ctr": "node src/server/generateServerKey.js",
    "start": "NODE_ENV=development webpack serve --config config/dev.webpack.config.js",
    "build:prod": "NODE_ENV=production webpack --config config/prod.webpack.config.js",
    "deploy": "npm-run-all build:prod lint test",
    "verify": "npm-run-all build lint test",
    "nightly": "npm run deploy"
  },
  "routes": {
    "email": "/email"
  },
  "insights": {
    "appname": "user-preferences"
  }
}
