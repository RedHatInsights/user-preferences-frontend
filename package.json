{
  "name": "user-preferences-frontend",
  "version": "1.1.0",
  "engines": {
    "node": ">=15.0.0",
    "npm": ">=7.0.0"
  },
  "private": false,
  "dependencies": {
    "@babel/runtime": "^7.13.10",
    "@data-driven-forms/pf4-component-mapper": "^3.19.0",
    "@data-driven-forms/react-form-renderer": "^3.19.0",
    "@patternfly/react-core": "^4.264.0",
    "@patternfly/react-icons": "^4.93.0",
    "@patternfly/react-table": "^4.111.45",
    "@redhat-cloud-services/frontend-components": "^3.9.25",
    "@redhat-cloud-services/frontend-components-notifications": "^3.2.12",
    "@redhat-cloud-services/frontend-components-utilities": "^3.3.8",
    "classnames": "^2.3.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.9",
    "react-router-dom": "^5.2.0",
    "redux": "^4.0.5",
    "redux-logger": "^3.0.6",
    "redux-promise-middleware": "^6.1.2"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "@testing-library/jest-dom/extend-expect"
    ],
    "coverageDirectory": "./coverage/",
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/**/stories/*"
    ],
    "setupFiles": [
      "<rootDir>/config/setupTests.js"
    ],
    "roots": [
      "<rootDir>/src/"
    ],
    "moduleNameMapper": {
      "\\.(css|scss)$": "identity-obj-proxy"
    },
    "testURL": "http://localhost:5000/"
  },
  "devDependencies": {
    "@babel/core": "^7.13.10",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.13.8",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.13.10",
    "@babel/preset-env": "^7.13.10",
    "@babel/preset-flow": "^7.12.13",
    "@babel/preset-react": "^7.12.13",
    "@redhat-cloud-services/eslint-config-redhat-cloud-services": "^1.1.0",
    "@redhat-cloud-services/frontend-components-config": "^4.6.39",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^12.1.5",
    "axios-mock-adapter": "^1.19.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-plugin-lodash": "^3.3.4",
    "cross-fetch": "^3.1.0",
    "css-loader": "^5.1.3",
    "eslint": "^7.22.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-react": "^7.22.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.6.3",
    "npm-run-all": "^4.1.5",
    "prop-types": "^15.7.2",
    "redux-mock-store": "^1.5.4",
    "sass-loader": "^11.0.1",
    "stylelint": "^13.12.0",
    "stylelint-config-recommended-scss": "^4.2.0",
    "stylelint-scss": "^3.19.0",
    "webpack-bundle-analyzer": "^4.4.0"
  },
  "transformIgnorePatterns": [
    "/node_modules/(?!@redhat-cloud-services)"
  ],
  "scripts": {
    "build": "fec build",
    "test": "jest --verbose",
    "lint": "npm-run-all lint:*",
    "lint:js": "eslint config src",
    "lint:js:fix": "eslint config src --fix",
    "lint:sass": "stylelint 'src/**/*.scss' --config .stylelintrc.json",
    "server:ctr": "node src/server/generateServerKey.js",
    "start": "HOT=true fec dev",
    "deploy": "npm-run-all build lint test",
    "verify": "npm-run-all build lint test",
    "nightly": "npm run deploy"
  },
  "routes": {
    "notifications": "/notifications"
  },
  "insights": {
    "appname": "user-preferences"
  }
}
