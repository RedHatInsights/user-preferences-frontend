{
    "email-preference": {
        "advisor": {
            "title": "Advisor",
            "url": "/user-preferences/",
            "apiName": "insights",
            "permissions": [
                {
                    "method": "hasLoosePermissions",
                    "args": [[
                        "insights:*:*",
                        "insights:*:read",
                        "advisor:*:*",
                        "advisor:*:read",
                        "advisor:rules:read"
                    ]]
                }
            ]
        },
        "policies": {
            "title": "Policies",
            "permissions": [
              {
                  "method": "!isBeta"
              },
              {
                  "method": "hasPermissions",
                  "args": [["policies:*:*"]]
              }
            ]
        },
        "notifications": {
            "title": "Notifications (Beta)",
            "permissions": [
                {
                    "method": "isBeta"
                }
            ]
        },
        "unsubscribe": {
            "title": "",
            "localFile": "data/unsubscribe.json"
        }
    },
    "notification-preference": {
        "insights": {
            "title": "Insights",
            "application": "notifications",
            "resourceType": "notification-preference?bundleName=insights"
        }
    }
}
